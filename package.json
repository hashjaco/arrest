{
  "name": "arrest",
  "version": "1.0.0",
  "description": "CLI for creating, initializing, and building REST APIs with ease.",
  "main": "index.ts",
  "bin": {
    "arrest": "./dist/index.js"
  },
  "scripts": {
    "test": "test",
    "start": "ts-node src/index.ts",
    "pretty": "prettier --write '**/*.{js,json,ts}' '!node_modules/**'",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r src/templates dist"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hashjaco/arrest.git"
  },
  "keywords": [
    "[api",
    "rest",
    "cli]"
  ],
  "author": "Hashim Jacobs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hashjaco/arrest/issues"
  },
  "homepage": "https://github.com/hashjaco/arrest#readme",
  "dependencies": {
    "eslint-config-prettier": "^8.3.0",
    "inquirer": "^8.0.0"
  },
  "devDependencies": {
    "@types/inquirer": "^7.3.1",
    "@types/node": "^15.0.2",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "shx": "^0.3.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  }
}
